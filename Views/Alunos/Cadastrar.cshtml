@model Alunos //UTILIZANDO OS OBJETOS DA model Alunos

<h3>Cadastro de alunos</h3>

<form method="POST" asp-action = "Cadastrar">
    <H2>Informações dos alunos</H2>
    <p>
    <label>Nome do aluno</label>
    <input asp-for = "Nome">
    </p>
    <p>
    <label>Senha</label>
    <input asp-for = "Senha">
    </p>
    <p>
    <label>Usuario</label>
    <input asp-for = "Usuario">
    </p>
    <p>
    <label>Data Nascimento do aluno</label>
    <input asp-for = "DataNasc">
    </p>
    <input type="submit" value="Cadastrar">
</form>